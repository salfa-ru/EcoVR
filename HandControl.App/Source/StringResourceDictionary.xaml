<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=netstandard">

    <system:String x:Key="TitleControlPanel">Панель управления</system:String>
    
    <system:String x:Key="HeaderMainSettings">Основные настройки</system:String>
    <system:String x:Key="HeaderMovingSettings">Перемещение</system:String>
    <system:String x:Key="HeaderScrollingSettings">Скроллинг</system:String>
    <system:String x:Key="HeaderButtonSettings">Кнопки</system:String>

